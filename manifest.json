{
  "manifest_version": 2,
  "name": "Messenger",
  "version": "1.2",
  "author": "Maciej Muci3k Adach",
  "description": "Messenger umieszczony w sidebarze",
  "sidebar_action": {
    "default_panel": "sidebar.html",
    "browser_style": false,
    "default_icon": {
      "16": "icons/messenger_icon_16.png",
      "48": "icons/messenger_icon_48.png",
      "128": "icons/messenger_icon_128.png"
    }
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "sidebar.html",
    "default_icon": {
      "16": "icons/messenger_icon_16.png",
      "48": "icons/messenger_icon_48.png",
      "128": "icons/messenger_icon_128.png"
    }
  },
  "icons": {
    "16": "icons/messenger_icon_16.png",
    "48": "icons/messenger_icon_48.png",
    "128": "icons/messenger_icon_128.png"
  },
  "addons": {
    "{abcd1234-1abc-1234-12ab-abcdef123456}": {
      "updates": [
        {
          "version": "1.3",
          "update_link": "https://github.com/Muci3k/instagramSiDEBar.git"
        }
      ]
    }
  }
}
